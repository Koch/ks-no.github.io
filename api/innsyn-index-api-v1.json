{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS Fiks: Innsyn Index Service",
    "contact" : {
      "name" : "Fiks-brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.test.ks.no/"
  } ],
  "tags" : [ {
    "name" : "Index"
  } ],
  "paths" : {
    "/innsyn-index/api/v1/indexRequest" : {
      "put" : {
        "tags" : [ "Index" ],
        "summary" : "Indekser nye meldinger, eller oppdater eksisterende meldinger",
        "description" : "Batch av en eller flere meldinger, opp til et maksimum av 5000. Merk at operasjonen ikke er atomisk: responsen vil indikere om en eller flere indekseringsoperasjoner feilet.",
        "operationId" : "index",
        "requestBody" : {
          "description" : "Meldingene som skal indekseres",
          "content" : {
            "application/json" : {
              "schema" : {
                "minItems" : 1,
                "type" : "array",
                "items" : {
                  "$ref" : "#/components/schemas/IndexMeldingV1"
                }
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Indekseringsoperasjonen er gjennomført. Merk at indekseringen av en eller flere meldinger kan ha feilet, se de individuelle respons-kodene.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/MeldingIndexResponseV1"
                }
              }
            }
          },
          "422" : {
            "description" : "Antall meldinger i operasjonen overgår øvre begrensning (5000)"
          },
          "500" : {
            "description" : "Indekseringsoperasjonen har feilet. Et ukjent antall meldinger kan likevel ha blitt indeksert, men alle bør sendes inn på nytt"
          },
          "400" : {
            "description" : "Forespørselen er ikke korrekt formatert",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Valdieringsfeil"
                  }
                }
              }
            }
          },
          "403" : {
            "description" : "Integrasjonen er ikke autorisert for å indeksere på vegne av spesifisert organisasjon",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/IntegrasjonIkkeAutorisertForIndexApiModel"
                }
              }
            }
          }
        }
      }
    },
    "/innsyn-index/api/v1/slett" : {
      "delete" : {
        "tags" : [ "Index" ],
        "summary" : "Fjerner meldinger indekserte av den autentiserte integrasjonen for den spesifiserte fiks-organisasjonen. Ekstern-ref eller meldingsid kan legges til for ytterligere filtrering. Merk at dette starter en asynkron jobb, og det vil bli returnert på forespørselen før slettejobben faktisk er utført.",
        "operationId" : "slett",
        "parameters" : [ {
          "name" : "fiksOrganisasjonId",
          "in" : "query",
          "description" : "Fiks-organisasjon id",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        }, {
          "name" : "eksternRef",
          "in" : "query",
          "description" : "Ekstern referanse",
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "meldingId",
          "in" : "query",
          "description" : "Melding id",
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "202" : {
            "description" : "successful operation",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SlettRespons"
                }
              }
            }
          }
        }
      }
    },
    "/innsyn-index/api/v1/slettByMeldingId" : {
      "delete" : {
        "tags" : [ "Index" ],
        "summary" : "Fjerner meldinger for de spesifiserte melding-id'ene (maks 1000). Dette starter en synkron operasjon, det vil ikke returneres på forespørselen før slettingen er utført",
        "operationId" : "slettByMeldingId",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/SlettMeldingerSpec"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SlettRespons"
                }
              }
            }
          }
        }
      }
    }
  },
