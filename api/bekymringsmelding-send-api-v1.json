{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS FIKS: Bekymringsmelding send-service",
    "description" : "Bekymringsmelding send-service",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://www.ks.no/fagomrader/digitalisering/utviklingsprosjekter/",
      "email" : "fiks@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "bekymringsmelding-send",
    "description" : "API for å håndtere funksjonalitet vedr. bekymringsmelding"
  } ],
  "paths" : {
    "/bekymringsmelding/send/api/v1/send" : {
      "post" : {
        "tags" : [ "bekymringsmelding-send" ],
        "description" : "Sender en ordre som skal sendes videre til enten FiksIO eller Fiks-Print",
        "operationId" : "sendOrdre",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Ordre"
              }
            }
          }
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : { }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "FiksIOOrdre" : {
        "type" : "object",
        "properties" : {
          "mottakerKontoId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "meldingstype" : {
            "type" : "string"
          },
          "dokumentRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "ttl" : {
            "type" : "object",
            "properties" : {
              "seconds" : {
                "type" : "integer",
                "format" : "int64"
              },
              "negative" : {
                "type" : "boolean"
              },
              "zero" : {
                "type" : "boolean"
              },
              "units" : {
                "type" : "array",
                "items" : {
                  "type" : "object",
                  "properties" : {
                    "durationEstimated" : {
                      "type" : "boolean"
                    },
                    "dateBased" : {
                      "type" : "boolean"
                    },
                    "timeBased" : {
                      "type" : "boolean"
                    }
                  }
                }
              },
              "nano" : {
                "type" : "integer",
                "format" : "int32"
              }
            }
          }
        }
      },
      "Ordre" : {
        "type" : "object",
        "properties" : {
          "print" : {
            "$ref" : "#/components/schemas/PrintOrdre"
          },
          "fiksIO" : {
            "$ref" : "#/components/schemas/FiksIOOrdre"
          }
        }
      },
      "PrintOrdre" : {
        "type" : "object",
        "properties" : {
          "dokumentRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "forsideRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "leverandorRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "konteringsKode" : {
            "type" : "string"
          }
        }
      }
    }
  }
}