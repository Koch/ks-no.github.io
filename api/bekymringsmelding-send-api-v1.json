{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS FIKS: Bekymringsmelding send-service",
    "description" : "Bekymringsmelding send-service",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://www.ks.no/fagomrader/digitalisering/utviklingsprosjekter/",
      "email" : "fiks@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "bekymringsmelding-send",
    "description" : "API for å håndtere funksjonalitet vedr. bekymringsmelding"
  } ],
  "paths" : {
    "/bekymringsmelding/send/api/v1/send" : {
      "post" : {
        "tags" : [ "bekymringsmelding-send" ],
        "description" : "Sender en ordre som skal sendes videre til enten FiksIO eller Fiks-Print",
        "operationId" : "sendOrdre",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Ordre"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "default" : {
            "description" : "default response",
            "content" : {
              "application/json" : { }
            }
          }
        }
      }
    },
    "/bekymringsmelding/send/api/v1/konfigurasjon/{fiksOrgId}/{fortrolighetsNiva}/{fiksIOKontoId}" : {
      "get" : {
        "tags" : [ "bekymringsmelding-send" ],
        "description" : "Henter konfigurasjon for sending",
        "operationId" : "hentKonfigurasjon",
        "parameters" : [ {
          "name" : "fiksOrgId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        }, {
          "name" : "fortrolighetsNiva",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "enum" : [ "NORMAL", "UNTATT_OFFENTLIGHET" ]
          }
        }, {
          "name" : "fiksIOKontoId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "404" : {
            "description" : "Ingen konfigurasjon funnet for gitt fiksOrgId og fortrolighetsnivå"
          },
          "200" : {
            "description" : "Konfigurasjon funnet",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SendKonfigurasjon"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "FiksIOOrdre" : {
        "required" : [ "dokumentRef", "meldingstype", "mottakerKontoId" ],
        "type" : "object",
        "properties" : {
          "mottakerKontoId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "meldingstype" : {
            "type" : "string"
          },
          "dokumentRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "ttl" : {
            "type" : "integer",
            "format" : "int64"
          }
        }
      },
      "Ordre" : {
        "required" : [ "print" ],
        "type" : "object",
        "properties" : {
          "print" : {
            "$ref" : "#/components/schemas/PrintOrdre"
          },
          "fiksIO" : {
            "$ref" : "#/components/schemas/FiksIOOrdre"
          }
        }
      },
      "PrintOrdre" : {
        "required" : [ "dokumentRef", "forsideRef", "konteringsKode", "leverandorRef" ],
        "type" : "object",
        "properties" : {
          "dokumentRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "forsideRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "leverandorRef" : {
            "type" : "string",
            "format" : "uuid"
          },
          "konteringsKode" : {
            "type" : "string"
          }
        }
      },
      "FiksIoKonfigurasjon" : {
        "required" : [ "offentligNokkel" ],
        "type" : "object",
        "properties" : {
          "offentligNokkel" : {
            "type" : "string"
          }
        }
      },
      "PrintKonfigurasjon" : {
        "required" : [ "leverandorId", "offentligNokkel" ],
        "type" : "object",
        "properties" : {
          "leverandorId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "offentligNokkel" : {
            "type" : "string"
          }
        }
      },
      "SendKonfigurasjon" : {
        "type" : "object",
        "properties" : {
          "printKonfigurasjon" : {
            "$ref" : "#/components/schemas/PrintKonfigurasjon"
          },
          "fiksIoKonfigurasjon" : {
            "$ref" : "#/components/schemas/FiksIoKonfigurasjon"
          }
        }
      }
    }
  }
}