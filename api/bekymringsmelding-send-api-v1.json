{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "KS FIKS: Send-service",
    "description" : "Send-service",
    "contact" : {
      "name" : "Fiks brukerstøtte",
      "url" : "https://ks-no.github.io/",
      "email" : "fiks-utvikling@ks.no"
    },
    "version" : "v1"
  },
  "servers" : [ {
    "url" : "https://api.fiks.ks.no",
    "description" : "KS Fiks API host",
    "variables" : { }
  } ],
  "tags" : [ {
    "name" : "fiks-send",
    "description" : "API for å håndtere funksjonalitet for sending til Fiks IO/Print"
  } ],
  "paths" : {
    "/send/api/v1/send/{id}" : {
      "post" : {
        "tags" : [ "fiks-send" ],
        "description" : "Sender en bestemt ordre på nytt enten til FiksIO eller Fiks-Print basert på ID",
        "operationId" : "sendOrdreById",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "400" : {
            "description" : "Ordre er allerede levert"
          },
          "404" : {
            "description" : "Ingen ordre funnet"
          },
          "204" : {
            "description" : "Send ordre."
          }
        }
      }
    },
    "/send/api/v1/status" : {
      "post" : {
        "tags" : [ "fiks-send" ],
        "description" : "Oppdaterer status på en ordre",
        "operationId" : "oppdaterStatus",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/StatusModel"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "204" : {
            "description" : "Status oppdatert"
          },
          "400" : {
            "description" : "Feil meldingstype for ordre tilhørende angitt meldingId."
          }
        }
      }
    },
    "/send/api/v1/historikk/{ordreId}" : {
      "get" : {
        "tags" : [ "fiks-send" ],
        "description" : "Henter historikk for en ordre",
        "operationId" : "getHistorikkForOrdre",
        "parameters" : [ {
          "name" : "ordreId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "type" : "string",
            "format" : "uuid"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Historikk for ordre funnet",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/OrdreHistorikkModel"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "StatusModel" : {
        "required" : [ "fiksIoKonto", "meldingId", "meldingstype" ],
        "type" : "object",
        "properties" : {
          "meldingId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "meldingstype" : {
            "type" : "string"
          },
          "fiksIoKonto" : {
            "type" : "string",
            "format" : "uuid"
          }
        }
      },
      "OrdreHistorikkModel" : {
        "required" : [ "ordreId", "state", "tidspunkt" ],
        "type" : "object",
        "properties" : {
          "ordreId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "tidspunkt" : {
            "type" : "string"
          },
          "meldingstype" : {
            "type" : "string"
          },
          "meldingId" : {
            "type" : "string",
            "format" : "uuid"
          },
          "state" : {
            "type" : "string"
          },
          "fiksIoKonto" : {
            "type" : "string",
            "format" : "uuid"
          },
          "feilmelding" : {
            "type" : "string"
          }
        }
      }
    }
  }
}