              "schema" : {
                "$ref" : "#/components/schemas/Bekymringsmelding"
              }
            }
          },
          "required" : true
        },
        "responses" : {
          "200" : {
            "description" : "Opprettet ny bekymringsmelding.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "string",
                  "format" : "uuid"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "Adresse" : {
        "required" : [ "adresselinje1", "poststed" ],
        "type" : "object",
        "properties" : {
          "adresselinje1" : {
            "type" : "string"
          },
          "adresselinje2" : {
            "type" : "string"
          },
          "adresselinje3" : {
            "type" : "string"
          },
          "postnummer" : {
            "pattern" : "^[0-9]{4}$",
            "type" : "string"
          },
          "poststed" : {
            "type" : "string"
          }
        }
      },
      "Barn" : {
        "required" : [ "personnavn" ],
        "type" : "object",
        "properties" : {
          "personnavn" : {
            "type" : "string"
          },
          "antattAlder" : {
            "type" : "string"
          },
          "adresse" : {
            "$ref" : "#/components/schemas/Adresse"
          }
        }
      },
      "Bekymringsmelding" : {
        "required" : [ "barn", "kommunenavn", "melder", "melding" ],
        "type" : "object",
        "properties" : {
          "kommunenummer" : {
            "pattern" : "^\\d{4}$",
            "type" : "string"
          },
          "kommunenavn" : {
            "type" : "string"
          },
          "bydelsnummer" : {
            "type" : "string"
          },
          "bydel" : {
            "type" : "string"
          },
          "melder" : {
            "$ref" : "#/components/schemas/Melder"
          },
          "relasjon" : {
            "type" : "string"
          },
          "barn" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/Barn"
            }
          },
          "melding" : {
            "type" : "string"
          }
        }
      },
      "Melder" : {
        "required" : [ "etternavn", "fornavn" ],
        "type" : "object",
        "properties" : {
          "fnr" : {
            "pattern" : "^[0-9]{11}$",
            "type" : "string"
          },
          "fornavn" : {
            "type" : "string"
          },
          "mellomnavn" : {
            "type" : "string"
          },
          "etternavn" : {
            "type" : "string"
          },
          "telefonnummer" : {
            "pattern" : "^[+]?[0-9]+([- ][0-9]+)*$",
            "type" : "string"
          },
          "skjultIdentitet" : {
            "type" : "boolean"
          },
          "adresse" : {
            "$ref" : "#/components/schemas/Adresse"
          }
        }
      }
    }
